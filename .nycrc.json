{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "lib/**/*"
    ],
    "exclude": [
      "lib/**/*.unit.ts",
      "lib/**/*shims.ts",
      "lib/**/*types.ts"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary",
      "lcov"
    ],
    "branches": 75,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }